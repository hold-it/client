{
    "manifest_version": 3,
    "name": "Objection!",
    "version": "1.0.0",
    "action": { "default_popup": "index.html" },
    "browser_action": {},
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "tabs"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
      }
    ]
  }
